{
  "name": "react-ssr-prisma-typescript-docker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "prettify": "prettier . --write --loglevel silent",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:css": "stylelint './src/**/*.styled.ts'",
    "check-types": "tsc --noemit",
    "assets:all": "yarn assets:icons && yarn assets:illustrations",
    "assets:icons": "npx @svgr/cli --config-file svgr.icons.config.js --out-dir ./src/icons ./src/assets/icons",
    "assets:illustrations": "npx @svgr/cli --config-file svgr.illustrations.config.js --out-dir ./src/illustrations ./src/assets/illustrations"
  },
  "dependencies": {
    "@mobily/ts-belt": "^3.13.1",
    "@next/font": "13.1.6",
    "@prisma/client": "4.9.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "axios": "^1.3.3",
    "cheerio": "^1.0.0-rc.12",
    "chrome-aws-lambda": "^10.1.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "next": "13.1.6",
    "puppeteer": "^19.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swiper": "^9.0.5",
    "typescript": "4.9.5",
    "react-i18next": "^12.1.5",
    "sharp": "^0.31.3",
    "styled-components": "^5.3.6",
    "i18next": "^22.4.9",
    "next-i18next": "^13.1.5",
    "@tanstack/react-query": "^4.24.6"
  },
  "devDependencies": {
    "prisma": "4.9.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@svgr/webpack": "^6.5.1",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "postcss-scss": "^4.0.6",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.4",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "yarn lint:fix --quiet"
    ],
    "./**/*.{ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "*.styled.{ts,tsx}": [
      "stylelint './src/**/*.styled.ts'"
    ]
  }
}
